{
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "~/mcp-dir",
        "~/.claude",
        "~/dev/ai-assistants/"
      ]
    },
    "dash-api": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/Kapeli/dash-mcp-server.git",
        "dash-mcp-server"
      ]
    },
    "synx_mcp_fps": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://syn-executor.wasmer.app/mcp"
      ]
    },
    "cv_mcp_fps": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://fps-cv.onrender.com/mcp"
      ]
    },
    "linkedin_mcp_fps": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://linkedin-mcp-fps.onrender.com/mcp"
      ]
    }
  },
  "preferences": {
    "quickEntryDictationShortcut": "capslock",
    "coworkScheduledTasksEnabled": false,
    "sidebarMode": "code"
  }
}